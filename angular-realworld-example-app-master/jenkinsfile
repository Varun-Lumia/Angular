pipeline {
    agent any
	
	tools{nodejs('Node JS 14')}
	
    stages {
		    
        stage('NPM Install') {
            steps {
                echo 'NPM Version'
				bat "npm -v"
            
        }
		}
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}