        stage('NPM Install') {
            steps {
                echo ''
				nodejs('Node JS 14') {
				npm -v
            }
        }
		}
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}